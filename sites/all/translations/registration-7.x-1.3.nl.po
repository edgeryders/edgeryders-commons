msgstr "%type_name: Nieuw registratie aanmaken"
msgid "%type_name: Edit own registrations"
msgstr "%type_name: Eigen registraties bewerken"
msgid "%type_name: Edit any registrations"
msgstr "%type_name: Elke registratie bewerken"
msgid "%type_name: Delete own registrations"
msgstr "%type_name: Eigen registraties verwijderen"
msgid "%type_name: Delete any registrations"
msgstr "%type_name: Elke registratie verwijderen"
